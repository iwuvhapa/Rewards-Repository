<h1>Listing Users</h1>

<% # erb block %>
<table border="0" cellpadding="4" cellspacing="1" bgcolor="#333333">
	<tr bgcolor="#CCCCCC">
<th>Login</th>
<th>Name</th>
<th>Email</th>
<th>Salon Name</th>
<th>Salon Address</th>
<th>City</th>
<th>State</th>
<th>Zip</th>
<th>Salon Owner</th>
<th>Telephone</th>
<th>License Number</th>
<th>Website</th>
<th>Distributor</th>
<th>Sales Consultant</th>
<th>Products Used</th>
<th>Number of Stylists</th>
<th>Number of Actions</th>
</tr>
<% for user in @users %>
<tr bgcolor="#CCCCCC">
<td><%=h user.login %></td>
<td><%=h user.name %></td>
<td><%=h user.email %></td>
<td><%=h user.salon_name %></td>
<td><%=h user.salon_address %></td>
<td><%=h user.city %></td>
<td><%=h user.state %></td>
<td><%=h user.zip %></td>
<td><%=h user.salon_owner %></td>
<td><%=h user.telephone %></td>
<td><%=h user.license_number %></td>
<td><%=h user.website %></td>
<td><%=h user.distributor %></td>
<td><%=h user.sales_consultant %></td>
<td><%=h user.products_used %></td>
<td><%=h user.number_of_stylists %></td>
<td><%=h user.number_of_stations %></td>
<td><%= link_to 'Show', user %></td>
<td><%= link_to 'Edit', edit_user_path(user) %></td>
<td><%= link_to 'Delete', user, :confirm => "Are you sure you want to delete this user?", :method => "delete"%></td>

</tr>
<% end %>
</table>